<p id="notice"><%= notice %></p>

<div class="w3-bar w3-large w3-theme-d4">
  <a href="#" class="w3-bar-item w3-button">
    <i class="fa fa-bars"></i>
  </a>

  <span class="w3-bar-item">Listing Projects</span>

  <a href="#" class="w3-bar-item w3-button w3-right">
    <i class="fa fa-search"></i>
  </a>
</div>

<div class="w3-container w3-content">
  <% @projects.each do |project| %>
    <div class="w3-panel w3-white w3-card-2 w3-display-container">
              <p><%= project.name %></p>
              <p><%= project.description %></p>
              <p><%= project.identifier %></p>
              <p><%= project.homepage %></p>
              <p><%= project.public %></p>
              <p><%= project.subprojectof %></p>
              <p><%= project.bugtracker %></p>
              <p><%= project.featuretracker %></p>
              <p><%= project.supporttracker %></p>
      
      <%= link_to 'Show', project %>
      <%= link_to 'Edit', edit_project_path(project) %>
      <%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
  <% end %>
</div>

<br>

<%= link_to 'New Project', new_project_path %>
